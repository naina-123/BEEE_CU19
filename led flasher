//DUAL PAIR LIGHT BLINKING 
void setup()
{
  for(int i=1;i<7;i++)
  {
  pinMode(i, OUTPUT);
  }
}

void loop()
{
  for(int j=1;j<7;j++)
  {
   if(j==6)
   {
       digitalWrite(6, HIGH);
  digitalWrite(1, HIGH);
  delay(500); 
  digitalWrite(6, LOW);
  digitalWrite(1, LOW);
  delay(500); 
   }
  else
  {
  digitalWrite(j, HIGH);
  digitalWrite(j+1, HIGH);
  delay(500); 
  digitalWrite(j, LOW);
  digitalWrite(j+1, LOW);
  delay(500);
  } 
  }
}
